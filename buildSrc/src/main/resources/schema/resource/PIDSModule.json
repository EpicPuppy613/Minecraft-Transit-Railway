		"SerializedDataBase"
	],
	"properties": {
		"type": {
			"type": "string"
		},
		"pos": {
			"type": "object",
			"properties": {
				"x": {
					"type": "number"
				},
				"y": {
					"type": "number"
				},
				"w": {
					"type": "number"
				},
				"h": {
					"type": "number"
				}
